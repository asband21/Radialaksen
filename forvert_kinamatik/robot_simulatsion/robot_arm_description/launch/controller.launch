<launch>

<rosparam file="$(find robot_arm_description)/launch/controller.yaml" command="load"/>
<node args="pitch_position_controller roll_position_controller elbow_position_controller shoulder_position_controller base_position_controller finger_gear_left_position_controller finger_gear_right_position_controller l2f_left_position_controller f2g_left_position_controller f2l_right_position_controller f2g_right_position_controller joint_state_controller " name="controller_spawner" ns="robot_arm" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/robot_arm/joint_states"/>
</node>

</launch>